{
  "expo": {
    "name": "Culturar",
    "slug": "culturar",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "culturar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.akinleyeoj.culturar"
    },
    "android": {
      "package": "com.akinleyeoj.culturar"
    },
    "web": {
      "bundler": "metro",
      "output": "single-page"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
