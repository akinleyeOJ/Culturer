{
  "expo": {
    "name": "Culturar",
    "slug": "culturar",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "culturar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.akinleyeoj.culturar",
      "associatedDomains": ["applinks:culturar.netlify.app"]
    },
    "android": {
      "package": "com.akinleyeoj.culturar",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "culturar.netlify.app",
              "pathPrefix": "/auth"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single-page"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}